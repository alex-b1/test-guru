<p>
  <strong><%= t('.header') %>: </strong>
  <%= @test.title %>
</p>

<p>
  <strong><%= t('.level') %>: </strong>
  <%= @test.level %>
</p>

<table>
  <thead>
  <th><%= t('.table_header') %>:</th>
  <th colspan="3"><%= t('.table_action') %>:</th>
  </thead>
  <%= render partial: 'admin/questions/question', collection: @test.questions %>
</table>

<p><%= link_to t('.to_questions'), admin_test_questions_path(@test.id) %></p>

<p><%= link_to t('element.link.edit'), edit_admin_test_path %></p>
<p><%= link_to t('element.link.back'), admin_tests_path %></p>


